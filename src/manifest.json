{
  "name": "imgcapture",
  "options_page": "options.html",
  "background": {
    "page": "background.html"
  },
  "browser_action": {
		"default_icon": "icon.png"
  },
  "manifest_version": 2,
	"content_scripts": [
    {
      "matches": ["<all_urls>"],
      "css": ["contentscript.css"],
      "js": ["contentscript.bundle.js"]
    }
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
